{
    "name": "ucsf-web-services/ucsf_8_pa",
    "description": "UCSF.edu Public Affairs Drupal 8 Migration",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "University of California, San Francisco",
            "email": "copyright@ucsf.edu",
            "role": "Copyright Owner"
        },
        {
            "name": "Eric Guerin",
            "email": "eric.guerin@ucsf.edu",
            "role": "Developer"
        },
        {
            "name": "Jayson Jaynes",
            "email": "jason.jaynes@ucsf.edu",
            "role": "Developer"
        }
    ],
    "conflict": {
        "drupal/core": "7.*"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "platform" : {
            "php" : "5.6.30"
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "ucdavis": {
            "type": "vcs",
            "url": "https://github.com/stopfstedt/sitefarm_seed"
        },
        "1": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.5",
        "oomphinc/composer-installers-extender": "^1.1",
        "wikimedia/composer-merge-plugin": "^1.4.1",
        "cweagans/composer-patches": "^1.6",
        "drupal/core" : "^8.6.0",
        "drupal-composer/drupal-scaffold": "^2.5",
        "ucdavis/sitefarm_seed": "dev-crop-dependency-update",
        "drupal/acquia_lift": "~3.0",
        "drupal/acquia_connector": "^1.12",
        "drupal/acquia_contenthub": "^1.25",
        "drupal/address": "^1.2",
        "drupal/anchor_link": "^1.0",
        "drupal/admin_toolbar": "^1.22",
        "drupal/block_style_plugins": "^1.0",
        "drupal/block_class" : "~1.0",
        "drupal/blazy": "^1.0@RC",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/conditional_fields": "^1.0",
        "drupal/ctools": "^3.0",
        "drupal/chosen": "^2.5",
        "drupal/colorbox": "^1.4",
        "drupal/config_update": "^1.5",
        "drupal/ckeditor_liststyle" : "^1.3",
        "drush/drush": "^9.3.0",
        "drupal/entity_block" : "^1.0",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.4",
        "drupal/fieldblock": "~2.0",
        "drupal/field_group": "^1.0",
        "drupal/google_analytics": "^2.2",
        "drupal/geofield": "^1.0@beta",
        "drupal/geofield_map": "^2.5",
        "drupal/geocoder": "^2.0@beta",
        "drupal/google_tag": "~1.0",
        "drupal/layout_plugin": "^1.0",
        "drupal/linkit": "~4.3",
        "drupal/twig_tweak": "^1.8",
        "drupal/features": "^3.7",
        "drupal/honeypot": "^1.2",
        "drupal/menu_block": "^1.4",
        "drupal/menu_breadcrumb": "^1.5",
        "drupal/menu_position": "~1.0",
        "drupal/menu_item_extras": "~2.0",
        "drupal/menus_attribute": "~1.1",
        "drupal/media_entity": "^2.0",
        "drupal/media_entity_instagram": "~2.0",
        "drupal/media_entity_twitter": "~2.0",
        "drupal/metatag" : "^1.5",
        "drupal/module_filter" : "^3.1",
        "drupal/notification": "^1.0",
        "drupal/redirect": "^1.0",
        "drupal/roleassign": "^1.0",
        "drupal/url_embed": "^1.0",
        "drupal/webform": "^5.0",
        "drupal/webform_views": "^5.0",
        "drupal/jsonapi" : "^1.23",
        "drupal/token" : "^1.3",
        "drupal/views_infinite_scroll" : "^1.5",
        "drupal/ckeditor_widgets": "~1.0",
        "drupal/paragraphs": "^1.3",
        "drupal/subpathauto": "~1.0",
        "drupal/migrate_plus": "^4.0",
        "drupal/migrate_tools": "^4.0",
        "drupal/node_class": "~1.0",
        "drupal/video_embed_field": "^2.0",
        "drupal/migrate_source_csv": "^2.0",
        "drupal/components": "~1.0",
        "drupal/image_url_formatter": "~1.0",
        "drupal/wbm2cm": "^2.0",
        "drupal/stage_file_proxy": "~1.0",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/gathercontent": "~4.0",
        "drupal/ckeditor_iframe": "~2.0",
        "drupal/scheduled_updates": "^1.0",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/scheduler": "^1.0",
        "drupal/slick": "^1.0",
        "drupal/slick_media": "^2.0",
        "drupal/slick_paragraphs": "1.0-rc1",
        "drupal/sitemap": "^1.3",
        "drupal/search_api": "^1.5",
        "drupal/search_api_solr": "^1.0",
        "drupal/simple_sitemap": "^2.1",
        "drupal/social_media_links": "^2.6",
        "drupal/smart_trim": "^1.1",
        "drupal/taxonomy_import": "^1.0",
        "drupal/entity_browser": "^2.0",
        "drupal/inline_entity_form": "^1.0",
        "drupal/menu_reference_render": "^2.0@alpha",
        "npm-asset/blazy": "^1.8",
        "npm-asset/chosen-js": "^1.8",
        "npm-asset/jquery-colorbox": "^1.6",
        "npm-asset/dropzone": "^5.5",
        "npm-asset/jquery.easing": "^1.4",
        "npm-asset/slick-carousel": "^1.8",
        "drupal/config_installer": "^1.8",
        "drupal/media_entity_image": "^1.3"
    },
    "require-dev": {
        "drupal/console" : "^1.8",
        "drupal/devel": "^1.2"
    },
    "extra": {
        "installer-types": [
            "drupal-custom-module",
            "drupal-custom-theme",
            "drupal-library",
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/drush/contrib/{$name}": ["type:drupal-drush"],
            "docroot/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset",
                "type:drupal-library"
            ]
        },
        "merge-plugin": {
            "include": [
                "docroot/profiles/contrib/sitefarm_seed/composer.json"
            ],
            "replace": false,
            "merge-extra": true,
            "merge-extra-deep": false,
            "merge-scripts": true
        },
        "enable-patching": true,
        "patches" : {
            "drupal/core" : {
            "2644468-88 - Multiple image upload breaks image dimensions" :
            "https://www.drupal.org/files/issues/2644468-88.patch"
            },
            "drupal/fieldblock": {
            "2810783 - PHP exception when placing block":
            "https://www.drupal.org/files/issues/php_exception_when-2810783-30.patch"
            },
            "drupal/field_group": {
            "2652642 - Allow to position the group in the advanced sidebar column":
            "https://www.drupal.org/files/issues/2652642-33.patch"
            },
            "drupal/maxlength": {
            "2794421 - Add Maxlength to Link title":
            "https://www.drupal.org/files/issues/2794421-6.patch"
            },
            "drupal/conditional_fields": {
            "2834358 - Uninitialized string offset: 0 in conditional_fields_evaluate_dependency()":
            "https://www.drupal.org/files/issues/conditional_fields-uninitialized_string_offset_in_conditional_fields_evaluate_dependency-2834358-11.patch"
            },
            "drupal/paragraphs": {
            "2764681 - Allow paragraph type deletion if content exists":
            "https://www.drupal.org/files/issues/2018-05-31/allow_paragraphs_deletion-2764681-88.patch"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "cp -a docroot/profiles/contrib/sitefarm_seed/libraries/. docroot/libraries/",
            "@renamelibraries"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "cp -a docroot/profiles/contrib/sitefarm_seed/libraries/. docroot/libraries/",
            "@renamelibraries"
        ],
        "rmvendor" : "rm -rf vendor/*",
        "rmprofiles" : "rm -rf docroot/profiles/contrib/*",
        "purge" : [
            "@rmvendor",
            "@rmprofiles",
            "@composer clear-cache"
        ],
        "renamelibraries" : [
          "rm -rf docroot/libraries/colorbox docroot/libraries/chosen",
          "cp -a docroot/libraries/jquery-colorbox docroot/libraries/colorbox",
          "cp -a docroot/libraries/chosen-js docroot/libraries/chosen"
        ]
    }
}
