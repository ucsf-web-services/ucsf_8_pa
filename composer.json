  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "docroot/drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "docroot/libraries/{$name}": [
        "type:bower-asset",
        "type:npm-asset",
        "type:drupal-library"
      ]
    },
    "merge-plugin": {
      "include": [
        "docroot/profiles/contrib/sitefarm_seed/composer.json"
      ],
      "replace": false,
      "merge-extra": true,
      "merge-extra-deep": false,
      "merge-scripts": true
    },
    "enable-patching": true,
    "patches": {
      "drupal/core": {
        "2644468-88 - Multiple image upload breaks image dimensions": "https://www.drupal.org/files/issues/2644468-88.patch",
        "2061377-328 - Allow image style to be selected in Text Editor's image dialog" : "https://www.drupal.org/files/issues/2018-09-19/2061377-328.patch",
        "UCSF FilterAlign - remove aggressive removal of classes" : "./patches/core-filteralign.patch",
        "UCSF FilterCaption - Fix decode entities on FilterCaption" : "./patches/core-filtercaption.patch",
        "2973706 - Styles dropdown configuration permit multiple tags" : "https://www.drupal.org/files/issues/2018-05-18/allow-multiple-elements-2973706-2.patch",
        "2492171 - Provide options to sanitize filenames" : "https://www.drupal.org/files/issues/2019-02-06/2492171-4-257_1.patch"
      },
      "drupal/field_group": {
        "2652642 - Allow to position the group in the advanced sidebar column": "https://www.drupal.org/files/issues/2018-07-26/2652642-57.patch"
      },
      "drupal/entity_embed": {
        "UCSF Entity Embed Caption Fix - Allows the HTML tags to come through from the WYSIWYG": "./patches/entity_embed_caption_fix.patch"
      },
      "drupal/applenews": {
        "Fix delete process of templates components": "https://www.drupal.org/files/issues/2019-02-05/applenews-fix-delete-process-of-templates-components-3025958-9.patch",
        "Add preview link when no channels are configured": "https://www.drupal.org/files/issues/2019-02-05/applenews-preview-with-no-api-3029632-4.patch",
        "Limit tags in html": "https://www.drupal.org/files/issues/2019-02-05/applenews-limit_tags_in_html-11333241-2.patch"
      },
      "drupal/entity_embed_link": {
        "Prevent Entity Embed Link from overriding entity_embed_container.html.yml file in the theme" : "./patches/entity_embed_link.patch"
      }
    },
    "patchLevel": {
       "drupal/core": "-p2"
    }
  },
  "scripts": {
    "remove-git-submodules" : "find . -mindepth 2 -type d -name .git | xargs rm -rf",
    "remove-core-gzip": "find ./docroot/core -name '*.gz' | xargs rm -f",
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "cp -a docroot/profiles/contrib/sitefarm_seed/libraries/. docroot/libraries/",
      "@renamelibraries",
      "@remove-git-submodules",
      "@remove-core-gzip"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "cp -a docroot/profiles/contrib/sitefarm_seed/libraries/. docroot/libraries/",
      "@renamelibraries",
      "@remove-git-submodules",
      "@remove-core-gzip"
    ],
    "rmvendor": "rm -rf vendor/*",
    "rmprofiles": "rm -rf docroot/profiles/contrib/*",
    "purge": [
      "@rmvendor",
      "@rmprofiles",
      "@composer clear-cache"
    ],
    "renamelibraries": [
      "rm -rf docroot/libraries/colorbox docroot/libraries/chosen",
      "cp -a docroot/libraries/jquery-colorbox docroot/libraries/colorbox",
      "cp -a docroot/libraries/chosen-js docroot/libraries/chosen"
    ]
  }
}
