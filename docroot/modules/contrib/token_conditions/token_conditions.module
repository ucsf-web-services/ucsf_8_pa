<?php

/**
 * @file
 * Contains token_conditions.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function token_conditions_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the token_conditions module.
    case 'help.page.token_conditions':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Creates conditions for Blocks based on Tokens') . '</p>';
      return $output;

    default:
  }
}
