{
  "name": "drupal-ckeditor_y3ti_plugins",
  "version": "2.1.0",
  "description": "Drupal CKEditor 5 CKeditor Y3ti Plugins",
  "author": "",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "watch": "webpack --mode development --watch",
    "build:ckeditor": "webpack --config webpack.config.ckeditor.js",
    "build:customElements": "webpack --mode production --config webpack.config.customElements.js",
    "build": "npm run build:ckeditor && npm run build:customElements",
    "uglify": "uglifyjs js/accordion.frontend.js -c -m --output js/accordion.frontend.min.js"
  },
  "dependencies": {
    "uglify-js": "^3.17.0"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-utils": "^30.0.0",
    "ckeditor5": "~34.1.0",
    "css-loader": "^7.1.1",
    "raw-loader": "^4.0.2",
    "terser-webpack-plugin": "^5.2.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.4.0"
  }
}
