{{ content.body }}
    {{ content.field_content_panel }}
    {% if content.field_first_media %}
      {% set classes = [
        'paragraph--type--gallery',
      ] %}
      {{ attach_library('ucsf/slick_gallery') }}
      <div class="paragraph-text-block gallery-background-fill"></div>
      <div{{ attributes.addClass(classes) }}>
        <div class="gallery-container">
          <div class="field-gallery-items">
          {{content.field_first_media}}
          </div>
        </div>
      </div>
      {{content.field_first_media}}
    {% endif %}
<div class="tags-menu-region">
  {% if content.field_topics|render or content.field_tags|render %}
    <h3 class="tags-menu__title">{{ "Topics"|t }}</h3>
    <ul class="tags-menu">
      {{ content.field_topics }} {{ content.field_tags }}
    </ul>
  {% endif %}
</div>
