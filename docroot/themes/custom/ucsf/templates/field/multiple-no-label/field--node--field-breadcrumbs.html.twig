{% extends 'field.html.twig' %}

{% block multiple_item_no_label %}
<nav aria-label="Breadcrumb" class="breadcrumb-region">
  <ol{{ attributes.addClass(classes, 'field__items') }}>
    {% for item in items %}<li{{ item.attributes.addClass('field__item') }}>{{ item.content }}</li>{% endfor %}{% block coda %}{% set entity = element['#object'] %}<li class="field__item">
        {{ entity.label }}
      </li>
    {% endblock coda %}
  </ol>
</nav>
{% endblock multiple_item_no_label %}
