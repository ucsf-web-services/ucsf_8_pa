{{ content.field_breadcrumbs }}

<main role="main" class="main" {{ attributes.addClass(classes) }}>
  <a id="main-content" tabindex="-1"></a>

  {% if node.field_banner_image is not empty %}
    {% set banner_image = "narrow-page-header-banner" %}

    <div class="article-medium-banner">
      {{ content.field_banner_image }}
    </div>
  {% endif %}
  <div class="main-content narrow-page">
    <header class="narrow-page-header {{ banner_image }}">
      <h1>{{ label }}</h1>
      <div class="narrow-page-header__subheading">{{ content.body }}</div>
    </header>
    {{ content.field_content_panel }}
  </div>
</main>

{% if content.field_footer_tabs_events_columne|render|striptags != ''  %}
<div class="narrow-page-full-width-wrapper">
  {{ content.field_footer_tabs_events_columne }}
</div>
{% endif %}

{% if content.field_ankle_cta|render|striptags != ''  %}
<div class="footer-ctas-wrapper">
  {{ content.field_ankle_cta }}
</div>
{% endif %}
