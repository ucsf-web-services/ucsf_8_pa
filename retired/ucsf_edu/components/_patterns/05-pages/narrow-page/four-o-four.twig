{% embed "@templates/_four-o-four.twig" with {
  menu_items: header.menu_items,
  footer_menu_items: footer.menu_items,
} %}


  {% block page_content %}
    {% include "@organisms/title/about-page-title.twig" with {
      heading_class: 'narrow-page-header',
      heading: 'Oops! We’re lost.',
    } %}

<div class="page_narrow">

  <div class="two-columns">
    <div class="content-column">
        <p>Looks like we can’t find the page you were looking for!</p>

        <p>While we fix our site, maybe one of the following resources will help you find what you need.</p>
    </div>

    <div class="content-column">
      {% include "@atoms/04-images/00-image/figure.twig" with {
        img_src: '../../images/about/four-o-four.jpg',
        img_alt: 'Doctors with patient',
      } %}
    </div>
  </div>

  <div class="home-search">
    <form class="home-search__form">
      <input type="text" name="search" id="search-q" class="home-search__form-input" placeholder="Search people, places, ideas" onclick="this.value='';" tabindex="2">
      <input type="submit" id="edit-submit" name="op" value="Search" class="home-search__form-submit">
    </form>
  </div>

  <h2 class="eyebrow-title">Top Links</h2>

  <div class="two-columns">
    <div class="content-column">
      <ul class="cta--list">
